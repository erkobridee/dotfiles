# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_size = 2
indent_style = tab

# [Makefile]
# indent_style = tab

[*.md]
indent_style = space

# [*.sh]
# indent_style = tab
